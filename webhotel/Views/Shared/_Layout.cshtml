<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - webhotel</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@600;700&family=Old+Standard+TT:ital,wght@0,400;0,700;1,400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <div class="container-hd">
            <input type="checkbox" id="burger-chk" class="hidden" />
            <label for="burger-chk" class="ciao-burger">
                <div class="ciao-burger-lign"></div>
                <div class="ciao-burger-lign"></div>
                <div class="ciao-burger-lign"></div>
            </label>
            <div class="logo-hd">
                <a href="">
                    <img src="/img/f9b20bcb-f3f1-4155-ac18-c5d04dad6f38.png" alt="" />
                </a>
            </div>
            <div class="btn-hd">
                <a href="~/room.html">
                    <button>BOOK NOW</button>
                </a>
            </div>
        </div>
        <div class="container-hd-list" style="left: -100%">
            <div class="head-hd">
                <div class="logo-hd-list">
                    <img src="/img/f9b20bcb-f3f1-4155-ac18-c5d04dad6f38.png" alt="" />
                </div>
                <div class="ciao-close"></div>
            </div>
            <div class="body-menu-hd">
                <div class="bloc-menu-mom"><a href="~/index.html">Home</a></div>
                <div class="bloc-menu-hd"><a href="~/room.html">our Rooms</a></div>
                <div class="bloc-menu-hd"><a href="~/about.html">About us</a></div>
                <div class="bloc-menu-hd"><a href="~/contact.html">contact us</a></div>
                <div class="bloc-menu-hd"><a href="~/cart.html">cart</a></div>
                <div class="bloc-menu-hd"><a href="~/login.html">Login</a></div>
            </div>
        </div>
    </header>
            @RenderBody()
    <footer>
        <section class="ft">
            <div class="container-ft">
                <div class="bloc-ft">
                    <h2>About</h2>
                    <ul>
                        <li><a href="">The Tribe</a></li>
                        <li><a href="">FAQ</a></li>
                        <li><a href="">Changes and cancellations policy</a></li>
                    </ul>
                </div>
                <div class="bloc-ft">
                    <h2>GASTRONOMY</h2>
                    <ul>
                        <li><a href="">Kin Toh</a></li>
                        <li><a href="">Tseen Ja</a></li>
                        <li><a href="">Imix Beach Club</a></li>
                        <li><a href="">Mantli</a></li>
                    </ul>
                    <h3>UH MAY</h3>
                    <ul>
                        <li><a href="">Jungle Cuisine</a></li>
                    </ul>
                </div>
                <div class="bloc-ft">
                    <h2>Wellness</h2>
                    <ul>
                        <li><a href="">Spa Menu</a></li>
                        <li><a href="">Guest Wellness Program</a></li>
                    </ul>
                </div>
                <div class="bloc-ft">
                    <h2>Fashion</h2>
                    <ul>
                        <li><a href="">ZAK IK</a></li>
                        <li><a href="">An Nikena</a></li>
                    </ul>
                </div>
                <div class="bloc-ft">
                    <h2>Experiences</h2>
                    <ul>
                        <li><a href="">Sunsets</a></li>
                        <li><a href="">City of Arts</a></li>
                    </ul>
                </div>
                <div class="bloc-ft">
                    <h2>ART</h2>
                    <ul>
                        <li><a href="">SFER IK</a></li>
                    </ul>
                </div>
                <div class="bloc-ft">
                    <h2>CERemonies</h2>
                    <ul>
                        <li><a href="">Weddings</a></li>
                        <li><a href="">Events</a></li>
                    </ul>
                </div>
            </div>
            <div class="crate-ft">
                <div class="bloc-map">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d658.7414029718104!2d109.24080395535702!3d13.773715579875235!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x316f6d76b0cfd083%3A0x3057b788842021f0!2sCIAO%20Altara%20Seaview%20Quy%20Nhon!5e0!3m2!1svi!2s!4v1695175785606!5m2!1svi!2s"
                            width="100%"
                            height="100%"
                            style="border: 0"
                            allowfullscreen=""
                            loading="lazy"
                            referrerpolicy="no-referrer-when-downgrade"></iframe>
                </div>
                <div class="bloc-ic">
                    <img src="https://assets-global.website-files.com/5d5418be2086e778643257ee/5d9decdc300bba3b659eda04_adults-only.png"
                         alt="" />
                </div>
            </div>
        </section>
    </footer>
    <script src="<%= HttpUtility.HtmlEncode(" https: //cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js") %>
        "
                    integrity="sha384-oBqDVmMz9ATKxIep9tiCxS/Z9fNfEXiDAYTujMAeBAsjFuCZSmKbSSUnQlmh/jp3"
                    crossorigin="anonymous"></script>
    <script src="~/js/room.js"></script>
    <script src="~/js/jsheader.js"></script>
    <script src="~/js/banner.js"></script>
    <script src="~/js/js.js"></script>
    <script src=""<%= HttpUtility.HtmlEncode("https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.min.js") %>"
            integrity="sha384-7VPbUDkoPSGFnVtYi0QogXtr74QeVeeIs99Qfg5YCF+TidwNdjvaKZX19NZ/e6oz"
            crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/8d297cad6b.js"
            crossorigin="anonymous"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
